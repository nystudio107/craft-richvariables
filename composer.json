{
  "name": "nystudio107/craft-richvariables",
  "description": "Allows you to easily use Craft Globals as variables in Rich Text fields",
  "type": "craft-plugin",
  "version": "1.0.31",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "richvariables",
    "redactor",
    "variables"
  ],
  "support": {
    "docs": "https://nystudio107.com/plugins/rich-variables/documentation",
    "issues": "https://nystudio107.com/plugins/rich-variables/support"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "nystudio107",
      "homepage": "https://nystudio107.com"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0",
    "nystudio107/craft-plugin-vite": "^1.0.20"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "nystudio107\\richvariables\\": "src/"
    }
  },
  "extra": {
    "name": "Rich Variables",
    "handle": "rich-variables",
    "changelogUrl": "https://raw.githubusercontent.com/nystudio107/craft-richvariables/v1/CHANGELOG.md",
    "class": "nystudio107\\richvariables\\RichVariables"
  }
}
